 <!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">
        <link href="https://fonts.googleapis.com/css?family=Crimson+Text:400,400i,700|Montserrat:400,700" rel="stylesheet">
        <link rel="stylesheet" href="css/app.css">
        <script src="https://unpkg.com/masonry-layout@4.1/dist/masonry.pkgd.min.js"></script>

        <!--[if lt IE 9]>
            <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
            <script>window.html5 || document.write('<script src="js/vendor/html5shiv.js"><\/script>')</script>
        <![endif]-->
    </head>
    <body class="page {{page_modifier}}">
        
        <header data-js="header" class="header header--full page__header">
            {% block header %}
            {% endblock %}
        </header>
        
        <main class="main page__main">
            {% block main %}
            {% endblock %}
        </main>
        
        <footer class="footer page__footer">

            <ul class="footer__nav">

                {% for page in pages %}
                    <li class="footer__nav-item" data-id={{page.attributes.id}}>
                        <span class="footer__nav-label title">Volgende Pagina</span>
                        <span class="footer__nav-divider">|</span>
                        <a href="#{{page.attributes.id}}" class="footer__nav-link title">{{page.attributes.title}}</a>
                    </li>
                {% endfor %}
                
            </ul>
            
    
            <img class="footer__logo" src="./images/maetland-logo-small.svg" alt="" width="35">

            {% block footer %}
                <script src="js/app.bundle.js" charset="utf-8"></script>

                {% if env == 'development' %}
                    <script>document.write('<script src="http://' + (location.host || 'localhost').split(':')[0] + ':35729/livereload.js?snipver=1"></' + 'script>')</script>
                {% endif %}
                
            {% endblock %}
        </footer>
    </body>
</html>