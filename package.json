{
  "name": "bdcreations",
  "version": "0.0.1",
  "description": "frontend for new bdcreations website",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build/* && gulp --env development && webpack -d --ENV=development",
    "build:prod": "rimraf dist/* && gulp --env production && webpack -p --ENV=production",
    "serve": "webpack-dev-server --inline --hot --open --content-base build/ --ENV=development",
    "serve:prod": "webpack-dev-server --inline --hot --open --content-base build/ --ENV=production",
    "watch": "gulp watch --env development",
    "watch:prod": "gulp watch --env production",
    "init": "npm run build && npm run serve"
  },
  "keywords": [
    "bdcreations"
  ],
  "author": "Bob Donderwinkel",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "css-modularscale": "^1.1.1",
    "cssnano": "^3.7.6",
    "del": "^2.2.2",
    "event-stream": "^3.3.4",
    "front-matter": "^2.1.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-changed": "^1.3.2",
    "gulp-data": "^1.2.1",
    "gulp-fn": "0.0.0",
    "gulp-if": "^2.0.1",
    "gulp-image-resize": "^0.10.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-twig": "^0.5.0",
    "gulp-watch": "^4.3.10",
    "history": "^4.4.0",
    "jquery": "^3.1.1",
    "masonry-layout": "^4.1.1",
    "minimist": "^1.2.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-utilities": "^0.5.1",
    "reset-css": "^2.2.0",
    "rimraf": "^2.5.4",
    "slick-carousel": "^1.6.0",
    "slug": "^0.9.1",
    "underscore": "^1.8.3",
    "velocity-animate": "^1.2.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "yargs": "^6.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0"
  }
}
